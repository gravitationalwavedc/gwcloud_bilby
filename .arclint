{
  "linters": {
    "pyflakes": {
      "type": "pyflakes",
      "include": "(\\.py$)",
      "exclude": "(^react/|^venv/)"
    },
    "flake8": {
      "type": "flake8",
      "include": "(\\.py$)",
      "exclude": "(^react/|^venv/)",
      "severity": {
        "E501": "warning"
      }
    }
  }
}
